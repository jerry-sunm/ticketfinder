<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<div id="findId">
	<div id="section">
	    <div id="header">
	        <div id="headerArea">
	            <h1>
	                <img src="../images/logo.png">
	                <div class="finderBox">
	                    <a href="#">아이디 찾기</a>
	                    <a href="findPw.html" class="on">비밀번호 찾기</a>
	                </div>
	            </h1>
	        </div>
	    </div>
	    <div class="cont">
	    <form action="" method="post">
	        <div class="findIdForm">
	            <div>
	                <div class="info_input">
	                    <div class="info_Box">
	                        <div>
	                            <input type="text" id="name" class="name" placeholder="이름">
	                        </div>
	                        <div>
	                            <span><input type="email" id="email" class="email" placeholder="이메일 주소"></span>
	                            <span><a href="#" class="numBtn">인증번호 요청</a></span>
	                        </div>
	                        <div>
	                            <input type="text" id="num" class="num" placeholder="인증번호 (6자리)">
	                        </div>
	                    </div>
	                </div>
	            </div>
	            
	            <div class="wrap_btn">
	                <button type="submit" class="check_btn">확인</button>
	            </div>
	        </div>
	    </form>
	    </div>
	</div>
</div>
