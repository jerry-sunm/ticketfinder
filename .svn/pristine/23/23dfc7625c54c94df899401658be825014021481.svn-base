@charset "UTF-8";

/*----------------------------------------------
              mypage common style
----------------------------------------------*/
#mypage{
  margin-top: 50px;
}

#mypage .cont{
  width: 1100px;
}

#mypage>div{
  margin-bottom: 50px;
}

#mypage .titBox{
  width: 100%;
  padding: 0 10px;
  box-sizing: border-box;
  margin-bottom: 15px;
}
#mypage .titBox span{
  font-size: 20px;
}

#mypage .titBox a{
  float: right;
  font-size: 12px;
  margin-top: 7px;
}

#mypage a.btn{
  border: 1px solid #cccccc;
  color: #666;
  height: 18px;
  padding: 8px 12px;
  font-size: 12px;
}

#mypage a.btn:hover{
  border: 1px solid #000;
  color:#000;
}

/*----------------------------------------------
              mypage - section01 style
----------------------------------------------*/
#mypage #section01 .personWrapper{
  width: 100%;
  border: 1px solid #dddddd;
  background: #fafafa;
  overflow: hidden;
}

#mypage #section01 .personWrapper .personInfo{
  float: left;
  position: relative;
  width: 50%;
  padding: 24px 0 24px 24px;
  background: #fff;
  box-sizing: border-box;
}

#mypage #section01 .personWrapper .personInfo img{
  float: left;
  width: 100px;
  height: 100px;
  border-radius: 50%;
}

#mypage #section01 .personWrapper .personInfo .infoBox{
  float: left;
  width: 380px;
  height: 100px;
  margin-left: 20px;
}

#mypage #section01 .personWrapper .personInfo .infoBox p{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 25px;
  color: #333;
  line-height: 60px;
  max-width: 370px;
  padding: 0 4px;
  font-family: AppleSDGothicNeo-Regular, '맑은 고딕', 'Malgun Gothic';
}

#mypage #section01 .personWrapper .personInfo .infoBox a{
  margin-right: 10px;
}

#mypage #section01 .personWrapper .personState{
  float: right;
  position: relative;
  padding: 40px 0;
  overflow: hidden;
  width: 50%;
}

#mypage #section01 .personWrapper .personState li{
  float: left;
  width: 33.3333%;
  border-right: 1px solid #eeeeee;
  box-sizing: border-box;
}

#mypage #section01 .personWrapper .personState li a{
  font-size: 32px;
  line-height: 32px;
  text-align: center;
  display: block;
  color: #666;
}

#mypage #section01 .personWrapper .personState li p{
  margin-top: 11px;
  font-size: 13px;
  text-align: center;
}

/*----------------------------------------------
              mypage - section02 style
----------------------------------------------*/
#mypage #section02 .ticketBox table{
  border-top: 1px solid #e9e9e9;
  width: 100%;
  border-collapse: collapse;
  border-spacing: 0;
}

#mypage #section02 .ticketBox table tr td:first-child,
#mypage #section02 .ticketBox table tr td:last-child{
  width: 15%;
  text-align: center;
}

#mypage #section02 .ticketBox table tr td:nth-child(2){
  width: 40%;
}

#mypage #section02 .ticketBox table tr td:nth-child(3){
  width: 30%;
}

#mypage #section02 .ticketBox table tr{
  border-bottom: 1px solid #e9e9e9;
}

#mypage #section02 .ticketBox table thead td{
  text-align: center;
  font-size: 14px;
  color: #333;
  line-height: 50px;
}

#mypage #section02 .ticketBox table tbody td{
  padding: 20px 10px;
  box-sizing: border-box;
}

#mypage #section02 .ticketBox table td.bookDate{
  font-size: 16px;
  color:#999;
}

#mypage #section02 .ticketBox table td.bookShow img{
  width: 120px;
  float:left;
  margin-right: 30PX;
}

#mypage #section02 .ticketBox table td.bookShow p{
  float: left;
  max-width: 260px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

#mypage #section02 .ticketBox table td.bookShow .showTit{
  margin-top: 15px;
  margin-bottom: 75px;
  font-size: 18px;
}

#mypage #section02 .ticketBox table td.bookShow .showDate,
#mypage #section02 .ticketBox table td.bookShow .showPlace{
  color:#999;
  font-size: 14px;
  margin-bottom: 10px;
}

#mypage #section02 .ticketBox table td.bookDetail dl{
  width: 100%;
}

#mypage #section02 .ticketBox table td.bookDetail dl dt,
#mypage #section02 .ticketBox table td.bookDetail dl dd{
  float: left;
  margin-bottom: 10px;
  font-size: 15px;
  color:#999;
}

#mypage #section02 .ticketBox table td.bookDetail dl dt{
  width: 30%;
}

#mypage #section02 .ticketBox table td.bookDetail dl dd{
  width: 70%;
}

#mypage #section02 .ticketBox table td.bookState p{
  font-size: 15px;
  margin-bottom: 15px;
}

#mypage #section02 .ticketBox table td.bookState a{
  font-size: 13px;
  padding: 8px 12px;
}

#mypage #section02 .noTicketBox{
  border:1px solid #e9e9e9;
}

#mypage #section02 .noTicketBox p{
  line-height: 250px;
  color: #333;
  font-size: 20px;
  text-align: center;
}

/*----------------------------------------------
              mypage - section03 style
----------------------------------------------*/
#mypage #section03 .cont>div{
  width: 48%;
}

#mypage #section03 .cont>div .box{
  border: 1px solid #eee;
  padding: 4px 20px;
  margin-top: 4px;
  height: 232px;
  position: relative;
}


#mypage #section03 .review{
  float:left;
}

#mypage #section03 .inquiry{
  float: right;
}

#mypage #section03 .cont>div .box ul{
  display: none;
}

#mypage #section03 .cont>div .box ul li{
  color:#666;
  padding: 16px 0 16px;
  border-bottom: 1px solid #eeeeee;
}

#mypage #section03 .cont>div .box ul li:last-child{
 border-bottom: none;
}

#mypage #section03 .cont>div .box ul li a{
  display: inline-block;
  width: 100%;
  color:#666;
  font-size: 14px;
  /* margin-bottom: 5px; */
}

#mypage #section03 .cont>div .box ul li a:hover{
  color: #000;
}

#mypage #section03 .cont>div .box ul li a:hover .qState{
  border:1px solid #000;
}

#mypage #section03 .cont>div .box ul li a>span{
  display: inline-block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

#mypage #section03 .cont>div .box ul li a .qTit{
  max-width: 80%;
  vertical-align: top;
  padding-right: 3px
}

#mypage #section03 .cont>div .box ul li a .qState{
  line-height: 20px;
  padding: 0 10px;
  margin-left: 10px;
  border:1px solid #929292;
  border-radius: 13px;
  font-size: 13px;
  transform: translateY(-3px);
}

#mypage #section03 .cont>div .box ul li .qDate{
  font-size: 13px;
  color: #888;
}

#mypage #section03 .cont>div .box ul li a .reviewShow{
  max-width: 25%;
}

#mypage #section03 .cont>div .box ul li a .reviewTit{
  max-width: 70%;
  margin-left: 15px;
}

#mypage #section03 .cont>div .box p{
  text-align: center;
  font-size: 20px;
  color:#333;
}

#mypage #section03 .review .box ul li a>span{
  margin-bottom: 6px;
}

#mypage #section03 .review .noReview{
  line-height: 232px;
}

#mypage #section03 .inquiry .noQna{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
}

#mypage #section03 .inquiry .noQna a{
  display: inline-block;
  line-height: 18px;
  padding: 8px 16px;
  margin-top: 10px;
}
