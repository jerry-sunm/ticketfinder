	<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
	
	<script type="text/javascript">
		var dataSet = [
			{ label: "콘서트", data: 13, color: "#650c88" },
			{ label: "뮤지컬", data: 17, color: "#177bbb" },
			{ label: "연극", data: 40, color: "#a6c600" },
			{ label: "클래식", data: 9, color: "#8669CC" },
			{ label: "전시", data: 21, color: "#f84f9a" }
		];
	
		$.plot('#flot-donut', dataSet, {
			series: {
				pie: {
					show: true,
					combine: {
					color: '#999',
					threshold: 0.1
					}
				}
			},
			legend: {
			show: false
			}
		});
		
		$.plot('#flot-donut2', dataSet, {
			series: {
				pie: {
					show: true,
					combine: {
					color: '#999',
					threshold: 0.1
					}
				}
			},
			legend: {
			show: false
			}
		});
		
		$('.multiple-item').slick({
			  infinite: true,
			  slidesToShow: 2,
			  slidesToScroll: 2,
			  arrows: true,
			  variableWidth: true,
			  adaptiveHeight: true,
			  dots: true,
			  autoplay: true,
			  autoplaySpeed: 3000
		});
	</script>
	
	
