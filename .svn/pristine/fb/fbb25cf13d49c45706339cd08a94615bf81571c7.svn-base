  font-size: 25px;
  margin-bottom: 90px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

#bookDetail #section01 dl{
  width: 35%;
  border: none;
}

#bookDetail #section01 dt{
  width: 30%;
  float:left;
  margin-bottom: 10px;
  font-size: 14px;
  line-height: 14px;
  color: #444;
  padding: 0;
  background: #fff;
}

#bookDetail #section01 dd{
  width: 70%;
  float: right;
  margin-bottom: 10px;
  font-size: 14px;
  line-height: 14px;
  color: #444;
}

/*----------------------------------------------
          bookDetail - section02 style
----------------------------------------------*/
#bookDetail #section02 dl{
  width: 100%;
  float: left;
}

#bookDetail #section02 dl dt{
  background: #eee;
  line-height: 50px;
  font-size: 13px;
  padding-left: 30px;
  box-sizing: border-box;
  color: #777;
}

#bookDetail #section02 dl dd{
  width: 80%;
  line-height: 50px;
  font-size: 13px;
  padding-left: 30px;
  box-sizing: border-box;
  color: #777;
}

/*----------------------------------------------
          bookDetail - section03 style
----------------------------------------------*/
#bookDetail #section03 dl span{
  display: inline-block;
}

#bookDetail #section03 dl .price{
  font-size: 18px;
}

#bookDetail #section03 dl dt{
  width: 25%;
}

#bookDetail #section03 dl dd{
  width: 75%;
}

#bookDetail #section03 dl dd>div{
  width: 33.3333%;
}

#bookDetail #section03 dl dd>div.fee{
  padding-right: 30px;
  box-sizing: border-box;
}

#bookDetail #section03 dl dd>div>.symbol{
  font-size: 30px;
  margin-right: 30px;
  vertical-align: 10px;
}

#bookDetail #section03 dl dd>div .sum{
  color:#2cb935;
}

/*----------------------------------------------
          bookDetail - section05 style
----------------------------------------------*/
#bookDetail #section05 table{
  border: 1px solid #ddd;
  width: 100%;
  border-collapse: collapse;
  border-spacing: 0;
}

#bookDetail #section05 table td{
  text-align: center;
}

#bookDetail #section05 table thead td{
  background: #eee;
  line-height: 40px;
  color: #777;
