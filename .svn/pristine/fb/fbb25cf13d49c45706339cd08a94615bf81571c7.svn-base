  margin-top: 10px;
}

/*----------------------------------------------
          bookDetail - section06 style
----------------------------------------------*/
#bookDetail #section06 .box{
  width: 100%;
  background: #eee;
  padding: 30px 20px;
  box-sizing: border-box;
}

#bookDetail #section06 .box ul li{
  font-size: 13px;
  line-height: 18px;
  color: #888;
  margin-bottom: 7px;
  padding: 0 0 0 13px;
  background: url("../images/common/dot.png") no-repeat 0 7px;
}

