}

@keyframes textAnimation {
  0% { transform: translateX(200px); opacity: 0;}
  100% { transform: translateX(0); opacity: 1; }
}

-moz-@keyframes textAnimation {
  0% { transform: translateX(200px); opacity: 0;}
  100% { transform: translateX(0); opacity: 1; }
}

-webkit-@keyframes textAnimation {
  0% { transform: translateX(200px); opacity: 0;}
