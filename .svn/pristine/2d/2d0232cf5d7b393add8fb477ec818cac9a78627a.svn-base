#main .section04{
  width: 100%;
  padding: 50px 0;
  border-bottom: 1px solid #ddd;
}

#main>div.section04:nth-child(7){
  border-bottom: none;
}

#main .section04 .popBox{
  width: 100%;
  min-width: 1300px;
  position: relative;
  padding: 0 2%;
  box-sizing: border-box;
}

#main .section04 .popBox ul{
  width: 100%;
  font-size: 0;
  text-align: center;
}

#main .section04 .popBox ul li{
  float:left;
  width: 15%;
  margin-left: 2%;
}

#main .section04 .popBox ul li:first-child{
  margin-left: 0;
}

#main .section04 .popBox ul li a{
  display: inline-block;
  width: 100%;
}

#main .section04 .popBox ul li a:hover img{
  transform: scale(1.08);
}

#main .section04 .popBox ul li a:hover p{
  font-weight: bold;
}

#main .section04 .popBox ul li a img{
  width: 100%;
  -webkit-transition: transform .3s ease;
  -moz-transition: transform .3s ease;
  -o-transition: transform .3s ease;
  transition: transform .3s ease;
}

#main .section04 .popBox ul li a .itemTxt{
  width: 100%;
  height: 53px;
  padding-top: 10px;
}

#main .section04 .popBox ul li a .itemTxt p{
  text-align: center;
  line-height: 53px;
  font-size: 16px;
  color: #333;
  padding: 0 15px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

