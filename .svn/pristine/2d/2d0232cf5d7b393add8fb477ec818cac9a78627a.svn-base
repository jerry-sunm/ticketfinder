@charset "UTF-8";

/*----------------------------------------------
                    reset
----------------------------------------------*/
*{margin: 0; padding: 0;}
body{font-size: 16px; font-family: 'Noto Sans KR', 'NanumBarunGothic', '맑은 고딕', 'Malgun Gothic', sans-serif;}
a{color:#000; text-decoration: none;}
ul, li{list-style: none;}

/*----------------------------------------------
                    common style
----------------------------------------------*/
[class*="left"], [class*="Left"]{float:left;}
[class*="right"], [class*="Right"]{float:right;}


.cont{width: 1200px; margin: 0 auto;}
.cont::after, .cont::before,
.popBox::after, .popBox::before{
  content:'';
  display: block;
  clear:both;
}

/*----------------------------------------------
                  header style
----------------------------------------------*/
#header{
  width: 100%;
  min-width: 1300px;
  padding: 20px;
  background-color: #fff;
  border-bottom: 1px solid #cccc;
  box-sizing: border-box;
  /* position: relative; */
  position: fixed;
  top:0;
  left:0;
  z-index: 10;
}

#header::after, #header::before{
  content: '';
  display: block;
  clear: both;
}

#header>div{
  display: inline-block;
  vertical-align: middle;
}

#header .nav_left img{width: 90px;}
#header .nav_left h1{font-size: 0;}

#header .nav_center{
  margin-left: 50%;
  transform: translateX(-75%);
}

#header .nav_center::after, #header .nav_center::before{
  content: '';
  display: block;
  clear: both;
}

#header div ul li{
  float:left;
}

#header div ul li a{
  line-height: 35px;
  margin: 0 13px;
  font-size: 17px;
  position: relative;
}

#header .nav_center ul li a span{
  display: block;
  background: #ec7d2c;
  width: 0;
  height: 3px;
  position: absolute;
  top: 25px;
  left: 0;
  transition: all .3s ease;
  -webkit-transition: all .3s ease;
  -moz-transition: all .3s ease;
  -o-transition: all .3s ease;
}

#header .nav_center ul li a:hover span{
  width: 100%;
}

#header .nav_right{
  /* position: absolute;
  top:20px;
  right:20px; */
}

#header .nav_right ul li{
  position: relative;
}

#header .nav_right ul li a{
  margin: 0 15px;
  position: static;
}

#header .nav_right ul li a .fas{
  transform: scale(1.3);
  -webkit-transform: scale(1.3);
  -moz-transform: scale(1.3);
  -o-transform: scale(1.3);
}

#header .nav_right ul li:first-child a .fas{
  transform: scale(1.5);
  -webkit-transform: scale(1.5);
  -moz-transform: scale(1.5);
  -o-transform: scale(1.5);
}

#header .nav_right ul li a span{
  font-size: 11px;
  color:#333;
  opacity: 0;
  display: block;
  position: absolute;
  top: 25px;
  left: 0;
  width: 100%;
  text-align: center;
  transition: opacity .3s ease;
  -webkit-transition: opacity .3s ease;
  -moz-transition: opacity .3s ease;
  -o-transition: opacity .3s ease;
}

#header .nav_right ul li a:hover span{
  opacity: 1;
}

#header .nav_right ul li .searchBox{
  position: absolute;
  right: 0;
  top:55px;
  width: 340px;
  padding: 10PX 20px 12px;
  background: #333;
  border-radius: 5px;
  display: none;
  z-index: 2;
}

#header .nav_right ul li .searchBox::before{
  content:'';
	position:absolute;
  top:-10px;
  right:10px;
  border-bottom:10px solid #333;
	border-left:10px solid transparent;
	border-right:10px solid transparent;
}

#header .nav_right ul li .searchBox input{
  width: 300PX;
  line-height: 30px;
  font-size: 18px;
  background: #333;
  color:#fff;
  outline: none;
  border-top:none;
  border-left: none;
  border-right: none;
  border-bottom: 3px solid #fff;
}

#header .nav_right ul li .searchBox span{
  display: inline-block;
  width: 35px;
  height: 35px;
  float:right;
  position: relative;
  cursor: pointer;
}

#header .nav_right ul li .searchBox span::after,
#header .nav_right ul li .searchBox span::before{
  content: '';
  position: absolute;
  top:16.5px;
  left:10px;
  background: #fff;
  width: 30px;
  height: 2px;
}

#header .nav_right ul li .searchBox span::after{
  transform: rotate(45deg);
}

#header .nav_right ul li .searchBox span::before{
  transform: rotate(-45deg);
}


/*----------------------------------------------
                  footer style
----------------------------------------------*/
#footer{
  width: 100%;
  border-top: 1px solid #ccc;
  padding: 30px 0;
}

#footer .footerLeft{
  width: 15%;
}

#footer .footerLeft img{
  width: 100%;
}

#footer .footerRight{
