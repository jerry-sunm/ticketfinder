/*----------------------------------------------
              moveTopBtn style
----------------------------------------------*/
.moveTopBtn{
  display: block;
  position: fixed;
  width: 50px;
  height: 50px;
  overflow: hidden;
  bottom: -50px;
  right: 0;
  transition: all 0.5s;
}

.moveTopBtn.on{
  transform: translateY(-50px);
}

.moveTopBtn.on:hover img {
    margin-top: -50px;
}

